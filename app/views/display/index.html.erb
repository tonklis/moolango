<div class="header">
	<div class="row">
		<div class="span2">
			<div style="width: 100%; height: 120px;">
			<%= image_tag "webpage/logo.png"%>
			</div>
		</div>
		<div class="span7">
			<div style="width: 100%; height: 120px;">
				<%= image_tag "webpage/banner_biplane.png" %>
			</div>
		</div>
		<div class="span3">
			<div class="row no-space">
				<div style="width: 100%; height: 150px;">
					<div class="span1">
						<div class="nav_buttons">
						<%= image_tag "webpage/twitter.png"%>
						</div>
					</div>
					<div class="span1">
						<div class="nav_buttons">
						<%= image_tag "webpage/facebook.png"%>
						</div>
					</div>
					<div class="span1">
						<div class="nav_buttons">
						<%= image_tag "webpage/blog.png"%>
						</div>
					</div>
					<div class="span1">
						<div class="nav_buttons">
						<%= image_tag "webpage/login.png"%>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="span6">
			<div class="landing_left_hemisphere">
				<iframe src="http://player.vimeo.com/video/41532013?title=0&amp;byline=0&amp;portrait=0&amp;color=ff9933" width="100%" height="100%" frameborder="0" webkitAllowFullScreen mozallowfullscreen allowFullScreen></iframe>
			</div>
		</div>
		<div class="span6">
			<div class="landing_right_hemisphere">
				<h1 class="landing_header">
					ENJOYABLE, AFFORDABLE, <br /> INSTANT LANGUAGE PRACTICE <br /> ABOUT STUFF YOU LOVE
				</h1>
				<div class="landing_body">
				<%= image_tag "webpage/ohyeah.png", :alt => "...oh yeah, and you can make money speaking your language too..." %>
				</div>
				<div style="width: 100%;">
					<div class="landing_begin_button">
						<%= image_tag "webpage/begin.png", :alt => "begin button"%>
					</div>
				</div>
			</div>
		</div>	
	</div>
	<div class="row">
		<div class="span12">
			<div class="landing_background_image">
			<%= image_tag "webpage/globe.png", :alt => "HTML globe footer"%>
			</div>
		</div>
	</div>
</div>
